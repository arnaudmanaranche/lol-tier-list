{
  "compilerOptions": {
    "sourceMap": true,
    "lib": [
      "dom",
      "esnext"
    ],
    "esModuleInterop": true,
    "target": "es6",
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "Contexts/*": [
        "src/contexts/*"
      ],
      "Components/*": [
        "src/components/*"
      ],
      "Utils/*": [
        "src/utils/*"
      ]
    },
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "custom.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
