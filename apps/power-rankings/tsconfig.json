{
  "extends": "../../tsconfig.base",
  "display": "Power Rankings",
  "compilerOptions": {
    "lib": ["DOM.Iterable", "ESNext", "DOM"],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": "./src",
    "paths": {
      "Assets/*": ["./svgs/*"],
      "Hooks/*": ["./hooks/*"],
      "Utils/*": ["./utils/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "custom.d.ts", "src/pages/_error.js"]
}
