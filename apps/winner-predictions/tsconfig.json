{
  "extends": "../../tsconfig.base",
  "display": "Winner Predictions",
  "compilerOptions": {
    "lib": ["DOM.Iterable", "ESNext", "DOM"],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
