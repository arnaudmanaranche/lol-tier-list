{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "yarn lint:prettier && yarn lint:eslint",
    "lint:eslint": "next lint --fix",
    "lint:prettier": "prettier --write --check --ignore-path=../../.prettierignore .",
    "start": "next start"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --cache --cache-location .next/cache/eslint/ --fix"
    ]
  },
  "dependencies": {
    "@tremor/react": "2.3.1",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.2.7"
  },
  "devDependencies": {
    "@lpr/types": "1.0.0",
    "@next/env": "13.1.6",
    "eslint": "8.34.0",
    "eslint-config-lpr": "1.0.0",
    "eslint-config-next": "13.1.6",
    "tailwind-config-lpr": "1.0.0"
  }
}
