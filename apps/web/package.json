{
  "name": "web",
  "version": "0.0.1",
  "scripts": {
    "_generate-types": "supabase gen types --lang=typescript --project-id $NEXT_PUBLIC_SUPABASE_ID --schema public > src/types/database.types.ts",
    "build": "next build",
    "postbuild": "next-sitemap",
    "clean": "rm -rf .next && rm -rf .turbo",
    "dev": "next dev",
    "generate:types": "dotenv -- yarn _generate-types",
    "lint": "next lint --fix",
    "lint:prettier": "prettier --write --check .",
    "start": "next start",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --cache --cache-location ../../.turbo/cache/eslint/ --fix"
    ]
  },
  "dependencies": {
    "@cronitorio/cronitor-rum-nextjs": "0.4.0",
    "@heroicons/react": "2.0.17",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-popover": "1.1.4",
    "@radix-ui/react-tabs": "1.1.2",
    "@supabase/ssr": "0.5.2",
    "@supabase/supabase-js": "2.47.3",
    "@vercel/analytics": "1.4.1",
    "@vercel/og": "0.4.0",
    "axios": "0.24.0",
    "clsx": "1.2.1",
    "framer-motion": "11.15.0",
    "html-to-image": "1.11.11",
    "next": "14.2.25",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "resend": "4.0.1",
    "sonner": "1.7.1",
    "swr": "2.2.5",
    "twitter-api-v2": "1.20.2"
  },
  "devDependencies": {
    "@babel/core": "7.26.9",
    "@babel/preset-env": "7.26.9",
    "@babel/preset-react": "7.26.3",
    "@babel/preset-typescript": "7.26.0",
    "@next/env": "13.5.8",
    "@svgr/webpack": "5.5.0",
    "@types/node": "20",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@types/tailwindcss": "3.1.0",
    "autoprefixer": "10.4.20",
    "dotenv-cli": "7.4.4",
    "eslint-config-next": "14.2.22",
    "next-sitemap": "4.2.3",
    "postcss": "8.4.49",
    "prettier-plugin-tailwindcss": "0.6.11",
    "tailwindcss": "3.4.17",
    "typescript": "5"
  }
}
