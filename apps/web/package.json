    "@sentry/tracing": "7.37.1",
    "@supabase/supabase-js": "1.30.0",
    "clsx": "1.2.1",
    "framer-motion": "7.0.0",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@lpr/types": "*",
    "@svgr/webpack": "5.5.0",
    "eslint-config-lpr": "workspace:*",
